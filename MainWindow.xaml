<Window x:Class="KayChatOverlayWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KayChatOverlayWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="45" Width="678" Background="#99000000" AllowsTransparency="True" WindowStyle="None" Topmost="True" HorizontalAlignment="Center">
    <Grid>
        <Label x:Name="messageLabel" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" FontSize="16"/>
        <Label x:Name="queueLabel" Content="Label" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Bottom" Foreground="White"/>
    </Grid>
</Window>
